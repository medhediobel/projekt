			<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
				<span class="navbar-toggler-icon"></span>
			</button>
			<div class="collapse navbar-collapse" id="navbarColor01">
				<ul class="navbar-nav mr-auto">

					{% if app.user %}

						<li class="nav-item">
							<a class="nav-link active" href="{{ path('planes') }}">Bodenordnungsverfahren</a>
						</li>
						
						<li class="nav-item">
							<a class="nav-link active" href="https://video-app-1814-3735-dev.twil.io?passcode=22600718143735">Videokonferenz</a>
						</li>
						<li class="nav-item">
							<a class="nav-link active" href="{{ path('app_einwendung_new') }}">Einwendung</a>
						</li>
						<li class="nav-item">
							<a class="nav-link active" href="{{ path('agenda') }}">Anhörungstermin nach §41</a>
						</li>
						<!-- 
						<li class="nav-item">
							<a class="nav-link" href="{{ path('calendar_index') }}">Termin</a>
						</li>
						-->
						<li class="nav-item">
							<a class="nav-link active" href="{{ path('contact') }}">Email</a>
						</li>


						<div class="text-right">
							<ul class="navbar-nav mr-auto">
								<li class="nav-item">
									<a class="nav-link active" href="">Hallo
										{{app.user.lastName}}</a>
								</li>

								<li class="nav-item">
									<a class="nav-link active" href="{{path('app_logout')}}">Abmelden</a>
								</li>
							</ul>
						</div>

					{% else %}

						<li class="nav-item">
							<a class="nav-link" href="{{path('app_login')}}">Anmelden</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="{{path('registration')}}">Registrieren</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="">Kontakt</a>
						</li>
					</ul>
				{% endif %}
			</div>
		</nav>

		<div class="container"> {% block body %}{% endblock %}
			</div>

	{% block footer %}
		<footer class="footer mt-auto py-3 bg-light">
	    <div class="container mt-4">
  		<div class="row">
  					<div class="col-md-8">
					  <div class="container">
						
							<a class="nav-link active" href="">Kontakt</a>
						
						</div>
						<div class="container">
						<a class="nav-link active" href="">Erklärung zur Barrierefreiheit</a>
						</div>
						<div class="container">
